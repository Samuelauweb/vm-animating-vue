urcesContent":["<template>\n  <div v-if=\"event\">\n    <h1>{{ event.title }}</h1>\n    <p>{{ event.time }} on {{ event.date }} @ {{ event.location }}</p>\n    <p>{{ event.description }}</p>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: ['id'],\n  created() {\n    this.$store.dispatch('fetchEvent', this.id)\n      // if fail, route to ErrorDisplay\n    .catch(error => {\n      this.$router.push({\n        name: 'ErrorDisplay',\n        params: { error: error }\n      })\n    })\n  },\n  computed: {\n    event() {\n      return this.$store.state.event\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}